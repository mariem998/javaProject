<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.AbonnementController">

    <VBox layoutX="-11.0" prefHeight="484.0" prefWidth="189.0" style="-fx-background-color: #0c2340; -fx-background-radius: 15;">
        <children>
            <ImageView fitHeight="127.0" fitWidth="189.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../images/19-02.png" />
                </image>
            </ImageView>
            <Label text="Hello," textFill="WHITE">
                <VBox.margin>
                    <Insets left="20.0" top="-47.0" />
                </VBox.margin>
                <font>
                    <Font name="Segoe UI Semibold" size="16.0" />
                </font>
            </Label>
            <Label fx:id="logged_user" text="user_name" textFill="WHITE">
                <font>
                    <Font name="Segoe UI Semibold Italic" size="19.0" />
                </font>
                <opaqueInsets>
                    <Insets right="5.0" />
                </opaqueInsets>
                <VBox.margin>
                    <Insets left="17.0" top="-3.0" />
                </VBox.margin>
            </Label>
            <Button fx:id="goToArticlesScene" mnemonicParsing="false" onAction="#goToArticlesScene" prefHeight="40.0" prefWidth="180.0" style="-fx-background-color: none;" text="Articles" textFill="WHITE">
                <font>
                    <Font name="Segoe UI" size="16.0" />
                </font>
                <graphic>
                    <Button mnemonicParsing="false" style="-fx-background-color: none;">
                        <effect>
                            <ImageInput x="10.0" y="10.0">
                                <source>
                                    <Image url="@../images/icons/article-16.png" />
                                </source>
                            </ImageInput>
                        </effect>
                        <padding>
                            <Insets left="13.0" right="21.0" top="19.0" />
                        </padding>
                    </Button>
                </graphic>
                <VBox.margin>
                    <Insets left="-33.0" top="51.0" />
                </VBox.margin>
            </Button>
            <Button fx:id="add_articles" layoutX="5.0" layoutY="87.0" mnemonicParsing="false" onAction="#goToAddArticlesScene" prefHeight="40.0" prefWidth="180.0" style="-fx-background-color: none;" text="Ajouter Articles" textFill="WHITE">
                <font>
                    <Font name="Segoe UI" size="16.0" />
                </font>
                <graphic>
                    <Button mnemonicParsing="false" style="-fx-background-color: none;">
                        <effect>
                            <ImageInput x="10.0" y="10.0">
                                <source>
                                    <Image url="@../images/icons/add-list-16.png" />
                                </source>
                            </ImageInput>
                        </effect>
                        <padding>
                            <Insets left="13.0" right="21.0" top="19.0" />
                        </padding>
                    </Button>
                </graphic>
                <VBox.margin>
                    <Insets left="-5.0" />
                </VBox.margin>
            </Button>
            <Button layoutX="5.0" layoutY="87.0" mnemonicParsing="false" onAction="#goToAddCategoriesScene" prefHeight="48.0" prefWidth="200.0" style="-fx-background-color: none;" text="Ajouter Catégorie" textFill="WHITE">
                <font>
                    <Font name="Segoe UI" size="16.0" />
                </font>
                <graphic>
                    <Button mnemonicParsing="false" style="-fx-background-color: none;">
                        <effect>
                            <ImageInput x="10.0" y="10.0">
                                <source>
                                    <Image url="@../images/icons/article-16.png" />
                                </source>
                            </ImageInput>
                        </effect>
                        <padding>
                            <Insets left="13.0" right="21.0" top="19.0" />
                        </padding>
                    </Button>
                </graphic>
                <VBox.margin>
                    <Insets left="-5.0" />
                </VBox.margin>
            </Button>
            <Button layoutX="5.0" layoutY="286.0" mnemonicParsing="false" onAction="#goToArticlesScene" prefHeight="48.0" prefWidth="220.0" style="-fx-background-color: none;" text="Abonnement" textFill="WHITE">
                <font>
                    <Font name="Segoe UI" size="16.0" />
                </font>
                <graphic>
                    <Button mnemonicParsing="false" style="-fx-background-color: none;">
                        <effect>
                            <ImageInput x="10.0" y="10.0">
                                <source>
                                    <Image url="@../images/icons/article-16.png" />
                                </source>
                            </ImageInput>
                        </effect>
                        <padding>
                            <Insets left="13.0" right="21.0" top="19.0" />
                        </padding>
                    </Button>
                </graphic>
                <VBox.margin>
                    <Insets left="-20.0" />
                </VBox.margin>
            </Button>
        </children>
    </VBox>
   <TableView fx:id="CategoriesExist" layoutX="217.0" layoutY="79.0" prefHeight="200.0" prefWidth="343.0">
     <columns>
         <TableColumn fx:id="CategoryTableColumnId" prefWidth="50.0" text="id" />
       <TableColumn fx:id="CategoryTableColumn" prefWidth="150.0" text="Catégories" />
     </columns>
   </TableView>

</AnchorPane>
